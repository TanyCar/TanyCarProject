<!DOCTYPE html>
<html>
    <head>
        <title> List Page</title>
    </head>
    <body>
        <h1>List Page</h1>
        <form method="post">
            <input type="text" name="model" value="<%data.model%>"/><input type="submit"/>
            <table width="100%" border="1">
                <tr>
                    <th>type</th>
                    <th>model</th>
                    <th>1hour Fee</th>
                    <th>go to rental</th>
                </tr>
                <% data.forEach(function(item,index){ %>
                <tr>
                    <td><%= item.type %></td>
                    <td><%= item.model %></td>
                    <td><%= item.modelID+item.drivingFEE %></td>
                    <td><a href="detail.ejs">예약하기</a></td>
                </tr>
                <% }); %>
            </table>
        </form>
    </body>
</html>